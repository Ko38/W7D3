<!DOCTYPE html>
<html>
  <head>
    <title>GoalApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
        Hello <%= current_user.user_name %>
    <% else %>
      Please Log in <a href="<%= new_session_url%>">HERE</a>
    <% end %>
    <%= yield %>
  </body>
</html>
